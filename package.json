{
  "name": "nextplate",
  "version": "5.5.0",
  "description": "Nextjs and Tailwindcss boilerplate",
  "author": "zeon.studio",
  "license": "MIT",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "dev": "concurrently \"node scripts/themeGenerator.mjs --watch\" \"yarn generate-json && next dev\"",
    "build": "node scripts/themeGenerator.mjs && yarn generate-json && next build",
    "preview": "next start",
    "postbuild": "next-sitemap",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier -w ./src",
    "generate-json": "node scripts/jsonGenerator.mjs",
    "remove-darkmode": "node scripts/removeDarkmode.mjs && yarn format"
  },
  "dependencies": {
    "@justinribeiro/lite-youtube": "^1.9.0",
    "@next/third-parties": "16.0.6",
    "date-fns": "^4.1.0",
    "disqus-react": "^1.1.7",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "marked": "^17.0.1",
    "next": "16.0.6",
    "next-mdx-remote": "^5.0.0",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-gtm-module": "^2.0.11",
    "react-icons": "^5.5.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "swiper": "^12.0.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.6",
    "postcss": "^8.5.6",
    "prettier": "^3.7.3",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "sharp": "0.34.5",
    "tailwind-bootstrap-grid": "^6.0.0",
    "tailwindcss": "^4.1.17",
    "typescript": "5.9.3"
  },
  "resolutions": {
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2"
  }
}
